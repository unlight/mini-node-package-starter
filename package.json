{
  "name": "mini-node-package-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "npm run eslint && npm run test:r",
    "test:r": "node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only ./node_modules/xv/lib/bin src/index.spec.ts",
    "test:w": "watchexec -w src \"npm run --silent test:r\"",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "build": "tsc"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "expect": "^27.4.6",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "watchexec-bin": "^1.0.0",
    "xv": "^1.1.1"
  }
}
