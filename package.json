{
  "name": "mini-node-package-starter",
  "version": "0.0.0-dev",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "test": "npm run eslint && npm run test:r",
    "test:r": "glob -c \"node --import tsx --test\" src/**/*.spec.ts",
    "test:w": "glob -c \"node --import tsx --test --watch\" src/**/*.spec.ts",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "esw -w --clear --ext ts,tsx,mts src",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "prebuild": "rm -rf dist",
    "build": "pkgroll --target=es2020"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-watch": "^8.0.0",
    "expect": "^29.7.0",
    "glob": "^10.3.10",
    "pkgroll": "^2.0.2",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.2.1",
    "request": "^2.88.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "watchexec-bin": "^1.0.0"
  }
}
